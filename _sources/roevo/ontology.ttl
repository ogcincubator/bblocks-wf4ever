@prefix ao: <http://purl.org/ao/> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ro: <http://purl.org/wf4ever/ro#> .
@prefix roevo: <http://purl.org/wf4ever/roevo#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://purl.org/wf4ever/roevo> a owl:Ontology ;
    dc:contributor "Jun Zhao"^^xsd:string,
        "Khalid Belhajjame"^^xsd:string,
        "Oscar Corcho"^^xsd:string,
        "Stian Soiland-Reyes"^^xsd:string ;
    dc:creator "Raul Palma"^^xsd:string ;
    dc:date "2014-01-15"^^xsd:string ;
    dc:description <http://www.wf4ever-project.org/wiki/download/attachments/2065027/ROevo_v0.4%2823%29.png>,
        """The Research Object Evolution Ontology (roevo) extends the ro ontology to capture the lifecycle and versioning of research objects (ROs). roevo is also an extension of PROV ontology for describing Research Object evolution provenance.
        roevo enables the representation of the different stages of the ROs lifecycle, their dependencies, as well as the corresponding versions of ROs and their aggregated resources, with the associated changes in these resources. 
        roevo follows a modular approach, enabling the creation of extensions for modeling detailed taxonomies of changes for particular resources, e.g., workflows.
        Additional information about roevo and background information is available at http://www.wf4ever-project.org/wiki/display/docs/RO+evolution"""^^xsd:string ;
    dc:rights "This ontology is distributed under Creative Commons Attribution 3.0 License - http://creativecommons.org/licenses/by/3.0"^^xsd:string ;
    dc:title "Research Object Evolution Ontology"^^xsd:string ;
    rdfs:comment """The Research Object Evolution Ontology (roevo) extends the ro ontology to capture the lifecycle and versioning of research objects. ro ontology and other extensions are described at http://wf4ever.github.io/ro/.
        roevo is also an extension of PROV ontology for describing Research Object evolution provenance. PROV ontology is described at http://www.w3.org/TR/prov-o/."""@en ;
    owl:versionIRI <http://purl.org/wf4ever/roevo/0.6.1> ;
    owl:versionInfo "0.6.1"^^xsd:string .

roevo:Addition a owl:Class ;
    rdfs:label "Addition" ;
    rdfs:comment "A type of roevo:Change that changes the aggregation, content and/or structure of the Research Object by adding a resources or an annotation" ;
    rdfs:subClassOf roevo:Change .

roevo:Modification a owl:Class ;
    rdfs:label "Modification" ;
    rdfs:comment "A type of roevo:Change that modifies the aggregation, content and/or structure of the Research Object by updating an aggregated resource content or an existing annotation" ;
    rdfs:subClassOf roevo:Change .

roevo:Removal a owl:Class ;
    rdfs:label "Removal" ;
    rdfs:comment "A type of roevo:Change that changes the aggregation, content and/or structure of the Research Object by removing a resources or an annotation" ;
    rdfs:subClassOf roevo:Change .

roevo:archivedAtTime a owl:DatatypeProperty ;
    rdfs:label "archivedAtTime" ;
    rdfs:comment "The time at which an roevo:ArchiveRO was completely created and is available for use" ;
    rdfs:domain roevo:ArchivedRO ;
    rdfs:range xsd:dateTime ;
    rdfs:subPropertyOf prov:generatedAtTime .

roevo:fromVersion a owl:ObjectProperty ;
    rdfs:label "fromVersion" ;
    rdfs:comment "Associates the set of changes (roevo:ChangeSpecification) that were used (applied) by the pointed version of the resource (roevo:VersionableResource)" ;
    rdfs:domain roevo:ChangeSpecification ;
    rdfs:range roevo:VersionableResource ;
    rdfs:subPropertyOf prov:used .

roevo:hasArchive a owl:ObjectProperty ;
    rdfs:label "hasArchive" ;
    rdfs:comment "Link a roevo:LiveRO with its archived version (roevo:ArchivedRO)" ;
    rdfs:domain roevo:LiveRO ;
    rdfs:range roevo:ArchivedRO ;
    owl:inverseOf roevo:isArchiveOf .

roevo:hasChange a owl:ObjectProperty ;
    rdfs:label "hasChange" ;
    rdfs:comment "Associates a roevo:Change to the roevo:ChangeSpecification including this change (in the set of changes generating a version of a resource)" ;
    rdfs:domain roevo:ChangeSpecification ;
    rdfs:range roevo:Change .

roevo:hasPreviousChange a owl:ObjectProperty ;
    rdfs:label "hasPreviousChange" ;
    rdfs:comment "Associates two roevo:Changes that occurred one after the other" ;
    rdfs:domain roevo:Change ;
    rdfs:range roevo:Change .

roevo:hasRevision a owl:ObjectProperty ;
    rdfs:label "hasRevision" ;
    rdfs:comment "Inverse of prov:wasRevisionOf, where a revision is a derivation for which the resulting entity is a revised version of some original. Hence, it associates an entity with another entity derived from the former." ;
    owl:inverseOf prov:wasRevisionOf .

roevo:hasSnapshot a owl:ObjectProperty ;
    rdfs:label "hasSnapshot" ;
    rdfs:comment "Link a roevo:LiveRO with its different snapshots (roevo:SnapshotRO)" ;
    rdfs:domain roevo:LiveRO ;
    rdfs:range roevo:SnapshotRO ;
    owl:inverseOf roevo:isSnapshotOf .

roevo:relatedResource a owl:ObjectProperty ;
    rdfs:label "relatedResource" ;
    rdfs:comment "Associates a change (roevo:Change) to the resource (roevo:VersionableResource) affected by this change" ;
    rdfs:domain roevo:Change ;
    rdfs:range roevo:VersionableResource ;
    rdfs:subPropertyOf prov:used .

roevo:snapshotedAtTime a owl:DatatypeProperty ;
    rdfs:label "snapshotedAtTime" ;
    rdfs:comment "The time at which an roevo:SnapshotRO was completely created and is available for use" ;
    rdfs:domain roevo:SnapshotRO ;
    rdfs:range xsd:dateTime ;
    rdfs:subPropertyOf prov:generatedAtTime .

roevo:toVersion a owl:ObjectProperty ;
    rdfs:label "toVersion" ;
    rdfs:comment "Associates the set of changes (roevo:ChangeSpecification) that generated the pointed version of the resource (roevo:VersionableResource)" ;
    rdfs:domain roevo:ChangeSpecification ;
    rdfs:range roevo:VersionableResource ;
    rdfs:subPropertyOf prov:generated .

roevo:wasArchivedBy a owl:ObjectProperty ;
    rdfs:label "wasArchivedBy" ;
    rdfs:comment "Indicates the prov:Agent responsible for creating the associated roevo:ArchivedRO" ;
    rdfs:domain roevo:ArchivedRO ;
    rdfs:range prov:Agent ;
    rdfs:subPropertyOf prov:wasAttributedTo .

roevo:wasChangedBy a owl:ObjectProperty ;
    rdfs:label "wasChangedBy" ;
    rdfs:comment "Associates a version of the resource (roevo:VersionableResource) to the set of changes (roevo:ChangeSpecification) that generated it" ;
    rdfs:domain roevo:VersionableResource ;
    rdfs:range roevo:ChangeSpecification ;
    rdfs:subPropertyOf prov:wasGeneratedBy .

roevo:wasSnapshotedBy a owl:ObjectProperty ;
    rdfs:label "wasSnapshotedBy" ;
    rdfs:comment "Indicates the prov:Agent responsible for creating the associated roevo:SnapshotRO" ;
    rdfs:domain roevo:SnapshotRO ;
    rdfs:range prov:Agent ;
    rdfs:subPropertyOf prov:wasAttributedTo .

prov:endedAtTime a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:domain prov:Activity ;
    rdfs:range xsd:dateTime .

prov:hadOriginalSource a owl:ObjectProperty ;
    rdfs:domain prov:Entity ;
    rdfs:range prov:Entity ;
    rdfs:subPropertyOf prov:wasDerivedFrom .

prov:startedAtTime a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:domain prov:Activity ;
    rdfs:range xsd:dateTime .

prov:wasAssociatedWith a owl:ObjectProperty ;
    rdfs:domain prov:Activity ;
    rdfs:range prov:Agent .

prov:wasQuotedFrom a owl:ObjectProperty ;
    rdfs:domain prov:Entity ;
    rdfs:range prov:Entity ;
    rdfs:subPropertyOf prov:wasDerivedFrom .

ao:annotatesResource a owl:ObjectProperty .

dcterms:identifier a owl:DatatypeProperty .

ro:AggregatedAnnotation a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty ao:annotatesResource ;
            owl:someValuesFrom [ a owl:Class ;
                    owl:unionOf ( roevo:Change roevo:ChangeSpecification roevo:VersionableResource ) ] ] .

ro:Resource a owl:Class .

roevo:isArchiveOf a owl:ObjectProperty ;
    rdfs:label "isArchiveOf" ;
    rdfs:comment "Inverse of roevo:hasArchive. It associates an roevo:ArchivedRO with the roevo:LiveRO from which it was created" .

roevo:isSnapshotOf a owl:ObjectProperty ;
    rdfs:label "isSnapshotOf" ;
    rdfs:comment "Inverse of roevo:hasSnapshot. It associates an roevo:SnapshotRO with the roevo:LiveRO from which it was created" .

prov:generated a owl:ObjectProperty .

prov:wasGeneratedBy a owl:ObjectProperty .

prov:wasRevisionOf a owl:ObjectProperty ;
    rdfs:domain prov:Entity ;
    rdfs:range prov:Entity ;
    rdfs:subPropertyOf prov:wasDerivedFrom .

roevo:LiveRO a owl:Class ;
    rdfs:label "LiveRO" ;
    rdfs:comment """A subclass of ro:ResearchObject representing a type of Research Object in live state. It is also a subclass of prov:Entity as this is a type of artifact with fixed aspects and for which we want to provide provenance. 

Live ROs  represent a work in progress. They are thus mutable as the content or state of their resources may change.""" ;
    rdfs:subClassOf ro:ResearchObject,
        prov:Entity .

prov:generatedAtTime a owl:DatatypeProperty .

prov:used a owl:ObjectProperty .

prov:wasAttributedTo a owl:ObjectProperty .

ro:ResearchObject a owl:Class .

prov:Agent a owl:Class .

prov:wasDerivedFrom a owl:AsymmetricProperty,
        owl:IrreflexiveProperty,
        owl:ObjectProperty ;
    rdfs:domain prov:Entity ;
    rdfs:range prov:Entity .

roevo:ArchivedRO a owl:Class ;
    rdfs:label "ArchivedRO" ;
    rdfs:comment """A subclass of ro:ResearchObject representing a type of Research Object in archival state. It is also a subclass of prov:Entity as this is a type of artifact with fixed aspects and for which we want to provide provenance. 

Archived ROs represent the final stage of a RO (release) where it has either reached a version that the author prescribes to be stable and meaningful and is appropriate for publication and long-term preservation, or it has been deprecated. They are therefore immutable, with no further changes or versions allowed""" ;
    rdfs:subClassOf ro:ResearchObject,
        prov:Entity .

roevo:SnapshotRO a owl:Class ;
    rdfs:label "SnapshotRO" ;
    rdfs:comment """A subclass of ro:ResearchObject representing a type of Research Object in snapshot state. It is also a subclass of prov:Entity as this is a type of artifact with fixed aspects and for which we want to provide provenance. 

Snapshot ROs are intended as a record of past activity, ready to be disseminated as a whole. They are immutable, and reflect the state of the Live RO at a certain time. """ ;
    rdfs:subClassOf ro:ResearchObject,
        prov:Entity .

roevo:ChangeSpecification a owl:Class ;
    rdfs:label "ChangeSpecification" ;
    rdfs:comment "A collection of roevo:Changes activities representing the set of changes in a Research Object between two different versions" ;
    rdfs:subClassOf prov:Activity .

roevo:VersionableResource a owl:Class ;
    rdfs:label "VersionableResource" ;
    rdfs:comment "A prov:Entity representing a type of artifact that may be versioned and for which we want to track its different versions. This class is equivalent to the union of roevo:SnapshotRO, roevo:ArchivedRO, ro:Resource and ro:AggregatedAnnotation that are the artifacts for which we want to track their versions." ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty dcterms:identifier ;
            owl:someValuesFrom rdfs:Literal ],
        prov:Entity ;
    owl:equivalentClass [ a owl:Class ;
            owl:unionOf ( ro:AggregatedAnnotation ro:Resource roevo:ArchivedRO roevo:SnapshotRO ) ] .

prov:Activity a owl:Class .

roevo:Change a owl:Class ;
    rdfs:label "Change" ;
    rdfs:comment "A prov:Activity that changes the Research Object (RO) aggregation, content, structure, or the annotations about the RO or its aggregated resources." ;
    rdfs:subClassOf prov:Activity .

prov:Entity a owl:Class .

